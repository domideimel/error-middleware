{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": [
      false
    ],
    "max-line-length": [
      true,
      120
    ],
    "member-access": [
      false
    ],
    "no-conditional-assignment": false,
    "no-namespace": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  }
}
